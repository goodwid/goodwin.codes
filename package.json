{
  "name": "goodwin.codes",
  "version": "0.0.1",
  "description": "Website for goodwin.codes",
  "main": "index.js",
  "repository": "git@github.com:goodwid/goodwin.codes.git",
  "author": "David Goodwin <david@goodwin.codes>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "init": "./acl-script/init",
    "lint:packages": "eslint packages/**/src",
    "lint:configs": "eslint *.js",
    "lint": "yarn run lint:packages && yarn run lint:configs",
    "lint:watch": "esw -w --clear packages/*/src",
    "start:admin": "webpack-dev-server --env.app=admin --hot --mode=development",
    "build:admin": "webpack --env.app=admin --mode=production",
    "build:admin:dev": "webpack --env.app=admin --progress --mode=development",
    "start:main": "webpack-dev-server --env.app=main --hot --mode=development",
    "build:main": "webpack --env.app=main --mode=production",
    "build:main:dev": "webpack --env.app=main --progress --mode=development",
    "test": "yarn workspaces run test -i",
    "test:watch": "yarn run test --watch",
    "deploy": "yarn run build:main && firebase deploy",
    "deploy:database": "firebase-bolt rtdb.rules.bolt && firebase deploy --only database"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jest-css-modules": "^2.0.0",
    "jest-prop-type-error": "^1.1.0",
    "prop-types": "^15.6.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-html-script-insert-loader": "^0.0.5"
  },
  "dependencies": {
    "dotenv-webpack": "^1.5.7",
    "firebase": "^7.14.0",
    "normalize.css": "^8.0.0",
    "react": "^16.13.1",
    "react-clipboard.js": "^2.0.16",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.1.1",
    "react-firebaseui": "^4.1.0",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2"
  }
}
